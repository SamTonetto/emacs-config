$1bool operator!=(const $2& x, const $2& y)
{
    return !(x == y);
}
$1bool operator>(const $2& x, const $2& y)
{
    return y < x;
}
$1bool operator<=(const $2& x, const $2& y)
{
    return !(y < x);
}
$1bool operator>=(const $2& x, const $2& y)
{
    return !(x < y);
